<div class="row">
	<div class="col-md-3">
		
	</div>
	<div class="col-md-9">
		<div class="row">
			<% @apps.each do |app| %>
				<div class="col-md-3">
					<%= image_tag app.picture,width: "100%"%> <br>
					<%=link_to app.name,app%> <br>					
					<%= star_rating_for app.rating_value %>(<%=app.review_number%>)
				</div>
			<% end %>
		</div>		
	</div>
</div>